<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CustomerAssets">
        <property name="idCustomerAssets" type="integer"/>
        <property name="fkSku" type="string"/>
        <property name="imageUrl" type="string"/>
        <property name="fkCustomer" type="integer"/>
        <property name="fkSalesOrderItem" type="integer"/>
        <property name="orderItem" type="Item"/>
        <property name="customer" type="Customer"/>
        <property name="product" type="ProductConcrete"/>
        <property name="createdAt" type="string"/>
        <property name="isDeleted" type="bool"/>
        <property name="updatedAt" type="string"/>
    </transfer>

    <transfer name="CustomerAssetsList">
        <property name="customerAssets" type="CustomerAssets[]"/>
        <property name="pagination" type="Pagination"/>
        <property name="idCustomer" type="integer"/>
    </transfer>

</transfers>
